<%= render "list", book: @books,  user: @user %>

<div class="col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class="table table-hover table-inverse">
        <tr>
            <th></th>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
        </tr>
    </table>
    <tbody>
      <% @user.each do |user| %>
        <tr>
            <td>
                <%= image_tag user.get_image(100,100) %>
            </td>

            <td>
              <%= user.name %>
            </td>

            <td>
                <%= link_to "Show", user_path(user.id) %>
            </td>
        </tr>
        <% end %>
    </tbody>
</div>